class Solution:
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        hashmap = {}
        for i, k in enumerate(nums):
            hashmap[k] = i
        for i, k in enumerate(nums):
            b = target - k
            if b in hashmap and hashmap.get(b) != i:
                return i, hashmap.get(b)
